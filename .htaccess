<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif||swf|css|js)$">
Header set Cache-Control "max-age=290304000, public"
</FilesMatch>


RewriteEngine On
RewriteBase /
RewriteCond %{HTTP_HOST} !(robinglauser.ch|localhost)$ [NC]
RewriteRule ^(.*)$ http://www.robinglauser.ch/$1 [L,R=301]
RewriteRule ^send /app.php
RewriteRule ^captcha /app.php
RewriteRule ^get /app.php